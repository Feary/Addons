<?xml version="1.1" ?>
<settings>
	<!--
		This file controls the settings for the Scoreboard plugin.
		Settings in the <global> section apply to all characters
		
		The available settings are:
		posX - x coordinate for position
		posY - y coordinate for position
		numPlayers - The maximum number of players to display damage for
		bgTransparency - Transparency level for the background. 0-255 range
	-->
	<global>
        <bgtransparency>200</bgtransparency>
        <display>
            <bg>
                <alpha>200</alpha>
                <blue>0</blue>
                <green>0</green>
                <red>0</red>
                <visible>true</visible>
            </bg>
            <flags>
                <bold>false</bold>
                <bottom>false</bottom>
                <italic>false</italic>
                <right>false</right>
            </flags>
            <padding>0</padding>
            <pos>
                <x>500</x>
                <y>100</y>
            </pos>
            <text>
                <alpha>255</alpha>
                <blue>255</blue>
                <font>Courier New</font>
                <fonts>
                </fonts>
                <green>255</green>
                <red>255</red>
                <size>8</size>
            </text>
        </display>
        <numplayers>8</numplayers>
        <posx>10</posx>
        <posy>250</posy>
        <resetfilters>true</resetfilters>
        <sbcolor>204</sbcolor>
        <showallidps>true</showallidps>
        <visible>true</visible>
	</global>
	<feary>
        <display>
            <bg>
                <blue>30</blue>
                <green>30</green>
                <red>30</red>
            </bg>
            <pos>
                <x>900</x>
                <y>300</y>
            </pos>
            <text>
                <size>10</size>
            </text>
        </display>
	</feary>
</settings>
